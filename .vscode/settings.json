{
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#CEEEEF",
    "activityBar.background": "#CEEEEF",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#25320e",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#CEEEEF",
    "statusBar.background": "#06666A",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#CEEEEF",
    "statusBarItem.remoteBackground": "#06666A",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#06666A",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#06666A99",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "biome.enabled": true,
  "search.useIgnoreFiles": true,
  "editor.codeActionsOnSave": {
    "source.addMissingImports": "explicit",
    "quickfix.biome": "explicit",
    "source.organizeImports.biome": "explicit"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "[javascript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  }
}
